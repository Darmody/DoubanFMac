  "description": "豆瓣FM for mac",
  "main": "main.js",
  "scripts": {
    "test": "NODE_ENV=test mocha -R spec --compilers js:babel-core/register --recursive app/**/*.spec.js",
    "coverage": "NODE_ENV=test istanbul cover _mocha --report lcovonly -- --compilers js:babel-core/register --recursive app/**/*.spec.js",
    "lint": "eslint app *.js",
    "dev-server": "node server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/config.production.js --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-dev": "cross-env HOT=1 NODE_ENV=development electron ./",
    "package": "cross-env NODE_ENV=production node package.js",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darmody/DoubanFMac.git"
